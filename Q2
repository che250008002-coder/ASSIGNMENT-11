#include <iostream>
#include <string>
using namespace std;

struct Student {
    string name;
    int age;
    char gender;
    string branch;
};

int main() {
    
    Student* students = new Student[100];
    Student* temp = new Student[200];
    for (int i = 0; i < 100; i++) {
        temp[i] = students[i];
    }
   delete[] students;
    students = temp;

    cout << "Enter details for 200 students:\n";

    for (int i = 0; i < 200; i++) {
        cout << "\nStudent " << i + 1 << endl;

        cout << "Name: ";
        cin >> students[i].name;

        cout << "Age: ";
        cin >> students[i].age;

        cout << "Gender: ";
        cin >> students[i].gender;

        cout << "Branch: ";
        cin >> students[i].branch;
    }

    cout << "200 students data entry completed" << endl;;
    delete[] temp;

    return 0;
}
	
